                <input asp-for="TestMode" class="form-control" />
                <span asp-validation-for="TestMode" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="DownloadNewPayments" class="control-label"></label>
                <input asp-for="DownloadNewPayments" class="form-control" />
                <span asp-validation-for="DownloadNewPayments" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="AddInvoice" class="control-label"></label>
                <input asp-for="AddInvoice" class="form-control" />
                <span asp-validation-for="AddInvoice" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="AddArticle" class="control-label"></label>
                <input asp-for="AddArticle" class="form-control" />
                <span asp-validation-for="AddArticle" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Save" class="btn btn-primary" />
                <a asp-action="Index" class="btn btn-primary">Back to List </a>

            </div>
        </form>
    </div>
</div>

